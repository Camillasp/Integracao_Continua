pipeline{
    agent any

    stages {
        stage('Clonar Repositorio') {
            steps {
                git branch: 'master', url: 'https://github.com/Camillasp/Automacao_UI_I.git'
            }
        }
        stage('Instalar Dependencias e Executar Testes') {
            steps {
                //Instala as dependencias
                sh 'npm install'
                //Executa os testes
                sh 'npx cypress run'
            }
        }

    }
}